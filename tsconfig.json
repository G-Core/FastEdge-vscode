{
  "compilerOptions": {
    "module": "Node16",
    "target": "ES2022",
    "outDir": "js-extension",
    "lib": ["ES2022"],
    "sourceMap": false,
    "rootDir": "src",
    "strict": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedParameters": true
  }
}
